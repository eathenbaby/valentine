{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/favicon.png",
      "dest": "/dist/public/favicon.png"
    },
    {
      "src": "/garden.html",
      "dest": "/client/public/garden.html"
    },
    {
      "src": "/garden-styles.css",
      "dest": "/client/public/garden-styles.css"
    },
    {
      "src": "/garden-script.js",
      "dest": "/client/public/garden-script.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.cjs"
    },
    {
      "src": "/health",
      "dest": "/dist/index.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.cjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
